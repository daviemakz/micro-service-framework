{
  "name": "micro-service-framework",
  "version": "1.0.0",
  "description": "This package allows streaming & non-streaming microservices to be build on the frontend and the backend.",
  "main": "index.js",
  "scripts": {
    "test": "npm test",
    "format": "prettier --config ./.prettierrc --write './**/*.js'",
    "format-windows": "prettier --config ./.prettierrc --write ./**/*.js",
    "deploy-patch": "git commit -a -m 'module update: patch' && git push && npm version patch && npm publish",
    "deploy-minor": "git commit -a -m 'module update: minor' && git push && npm version minor && npm publish",
    "deploy-major": "git commit -a -m 'module update: major' && git push && npm version major && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daviemakz/micro-services-framework.git"
  },
  "keywords": [
    "micro",
    "service",
    "framework",
    "browser",
    "webpack",
    "server",
    "server-end",
    "front-end",
    "node",
    "node",
    "react",
    "angular",
    "child-process"
  ],
  "author": "David Makuni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daviemakz/micro-services-framework/issues"
  },
  "homepage": "https://github.com/daviemakz/micro-services-framework#readme",
  "devDependencies": {
    "eslint": "^5.11.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-google": "^0.11.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.2",
    "mocha": "^5.2.0",
    "prettier": "^1.13.7",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1"
  },
  "dependencies": {
    "autobind": "^1.0.3",
    "babel-core": "^6.26.3",
    "browserify": "^16.2.2",
    "child-process": "^1.0.2",
    "child_process": "^1.0.2",
    "eslint-config-standard": "^12.0.0",
    "find-free-port": "^2.0.0",
    "fs": "0.0.2",
    "is-port-free": "^1.0.7",
    "is-running": "^2.1.0",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "net": "^1.0.2",
    "path": "^0.12.7",
    "sleep-async": "^1.0.5",
    "tmp": "0.0.33",
    "uuid": "^3.3.2"
  }
